.App {
  text-align: center;
  background-color: black;
  min-height: 100vh;
}

.App-logo {
  height: 40vmin;
  animation: App-logo-spin infinite 10s linear;
}

.django-logo {
	margin-top: 20vh;
	height: 40vmin;
  }

@keyframes App-logo-spin {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }
}
